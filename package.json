{
  "name": "styled-react-modal",
  "version": "2.0.0",
  "description": "A React modal built with styled-components.",
  "main": "build/index.js",
  "scripts": {
    "test": "jest --coverage --verbose",
    "format": "prettier --write {src,tests,examples}/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "start": "webpack --watch",
    "build": "webpack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderRichey/styled-react-modal.git"
  },
  "keywords": [
    "react-modal",
    "styled-components",
    "modal",
    "react"
  ],
  "tags": [
    "react",
    "modal",
    "styled-components"
  ],
  "author": "Alexander Richey <alexander.richey@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderRichey/styled-react-modal/issues"
  },
  "homepage": "https://github.com/AlexanderRichey/styled-react-modal#readme",
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/dom": "^6.0.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.6.0",
    "styled-components": "^4.3.2",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
